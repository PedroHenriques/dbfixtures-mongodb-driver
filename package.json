    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "dbfixtures": "^1.0.1",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.2",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "mongodb": "^3.1.10"
